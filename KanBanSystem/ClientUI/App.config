<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
    </startup>
    <system.serviceModel>
        <bindings>
            <wsDualHttpBinding>
                <binding name="WSDualHttpBinding_IBoardInteractionService" />
                <binding name="WSDualHttpBinding_IBoardManagerService" />
                <binding name="WSDualHttpBinding_IUserManagerService" />
            </wsDualHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/WcfServiceInteraction/BoardInteractionService/"
                binding="wsDualHttpBinding" bindingConfiguration="WSDualHttpBinding_IBoardInteractionService"
                contract="KrabServicesBoardInteraction.IBoardInteractionService"
                name="WSDualHttpBinding_IBoardInteractionService">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/WcfServiceInteraction/BoardManagerService/"
                binding="wsDualHttpBinding" bindingConfiguration="WSDualHttpBinding_IBoardManagerService"
                contract="KrabServicesBoardManager.IBoardManagerService" name="WSDualHttpBinding_IBoardManagerService">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/WcfServiceInteraction/UserManagerService/"
                binding="wsDualHttpBinding" bindingConfiguration="WSDualHttpBinding_IUserManagerService"
                contract="KrabServicesUserManager.IUserManagerService" name="WSDualHttpBinding_IUserManagerService">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>